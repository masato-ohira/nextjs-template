{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "ssg": "next build && next export -o dist",
    "serve": "cd dist && php -S localhost:3333",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "scraping": "node npm-script/scraping/index.js",
    "git": "git add . && git commit -a -m $M",
    "ftp": "M=deploy yarn git && git ftp push -s test -k",
    "deploy": "yarn ssg && yarn ftp"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-jsonp": "^1.0.4",
    "bulma": "^0.9.4",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "next-pwa": "^5.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-infinite-scroller": "^1.2.6",
    "react-inlinesvg": "^3.0.0",
    "react-markdown-it": "^1.0.2",
    "react-slick": "^0.29.0",
    "sleep-promise": "^9.1.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.5",
    "swiper": "^8.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.7.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-slick": "^0.23.10",
    "@types/uuid": "^8.3.4",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "typescript": "4.7.4"
  }
}
